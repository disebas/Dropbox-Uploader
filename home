
zenity --list \
     --title="Dropbox Uploader V(300.5)" \
     --text="Select an operation" \
     --width="480" \
     --height="350" \
     --column="Operation" --column="Description"\ ||
     "Upload" "Option to upload files" &&FILE=`zenity --file-selection --title="Select your file" ` &&./dropbox_uploader.sh upload $FILE





